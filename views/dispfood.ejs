<label>List of Food Products:</label>
  <ul class="foods">
  <% for(var i=0; i<foods.length; i++) {%>
    <li class="foods">
      <span><%= " ID: " +foods[i].foodid+"."%></span>
      <span><%=" Name: " + foods[i].foodname%></span>
      <span><%=" Price: " + foods[i].foodprice%></span>
      <span><%=" Origin: " + foods[i].foodorigin%></span>
      <span><%=" Description: " + foods[i].fooddesc%></span>
    </li>
  <% } %>
</ul>